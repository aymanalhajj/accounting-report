	<parameter name="P_WITH_MAIN_ACCS" class="java.math.BigDecimal"/>
	<query language="SQL"><![CDATA[SELECT
    ACCOUNT_ID,
	ACCOUNT_PARENT,
    DECODE($P{P_LANG_ID},1,ACCOUNT_NAME_EN,ACCOUNT_NAME_AR) ACCOUNT_NAME,
    DEBIT_BAL,
   CREDIT_BAL ,
   (SELECT 
DECODE(NVL($P{P_COST_CNTR_ID},0),0,'الكل', (SELECT DECODE($P{P_LANG_ID},1,NVL(COST_CTR_NAME_EN,COST_CTR_NAME_AR),	NVL(COST_CTR_NAME_AR,COST_CTR_NAME_EN))
	FROM ACC_COST_CENTER
WHERE  COST_CTR_ID = $P{P_COST_CNTR_ID}) )
	FROM DUAL) COST_CTR_NAME
FROM
    TABLE ( INCOME_STATEMENT(    
	                     P_COMPANY_ID  =>$P{P_COMPANY_ID},
                         P_TO_DATE =>  $P{P_TO_DATE},
						 P_FOR_MAIN_ACCS =>  $P{P_WITH_MAIN_ACCS},
                         P_COST_CNTR_ID => $P{P_COST_CNTR_ID}
            ) )]]></query>
