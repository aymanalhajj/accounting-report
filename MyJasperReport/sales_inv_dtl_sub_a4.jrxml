	<query language="SQL"><![CDATA[SELECT P."BARCODE",
	DECODE( $P{P_LANG_ID},1,P.PRODUCT_NAME_EN,P.PRODUCT_NAME_AR) PRODUCT_NAME,
	DECODE( $P{P_LANG_ID},1,U.UNIT_NAME_EN,U.UNIT_NAME_AR) UNIT_NAME,
	I."QUANTITY",
	ROUND(I.PRICE,3)  PRICE,
	ROUND(I.TOTAL_PRICE,3) TOTAL_PRICE,
	ROUND(I.VAT_PERCENTAGE,3) VAT_PERCENTAGE,
	ROUND(I.VAT_VALUE,3) VAT_VALUE,
	ROUND(I.TOTAL_AMOUNT,3) TOTAL_AMOUNT
FROM "SALES_INV_DTL" I
	JOIN "SALES_PRODUCT" P ON 
	 I."PRODUCT_ID" = P."PRODUCT_ID" 
	JOIN "SETUP_UNIT" U ON 
	 I."PRODUCT_UNIT_ID" = U."UNIT_ID" 
WHERE 
	 I."INVOICE_ID" = $P{P_INV_ID}]]></query>
