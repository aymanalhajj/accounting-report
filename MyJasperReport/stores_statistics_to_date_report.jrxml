	<query language="SQL"><![CDATA[SELECT 
	ROW_NUMBER() over(order by 1) seq,
	PRODUCT_NO,
	PRODUCT_BARCODE,
	DECODE( $P{P_LANG_ID},1,PRODUCT_NAME_EN,PRODUCT_NAME_AR) PRODUCT_NAME,
	DECODE( $P{P_LANG_ID},1,STORE_NAME_EN,STORE_NAME_AR) STORE_NAME,
	BAL,
	COST_AVG,
	COST_TOTAL
